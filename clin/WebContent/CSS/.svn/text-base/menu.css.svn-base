.bordaBox {
	background: transparent;
	width: 100%;
}

.bordaBox .b1,.bordaBox .b2,.bordaBox .b3,.bordaBox .b4,.bordaBox .b1b,.bordaBox .b2b,.bordaBox .b3b,.bordaBox .b4b
	{
	display: block;
	overflow: hidden;
	font-size: 1px;
}

.bordaBox .b1,.bordaBox .b2,.bordaBox .b3,.bordaBox .b1b,.bordaBox .b2b,.bordaBox .b3b
	{
	height: 1px;
}

.bordaBox .b2,.bordaBox .b3,.bordaBox .b4 {
	background: #356AA0;
	border-left: 0px solid #356AA0;
	border-right: 1px solid #356AA0;
}

.bordaBox .b1 {
	margin: 0 5px;
	background: #356AA0;
}

.bordaBox .b2 {
	margin: 0 3px;
	border-width: 0 2px;
}

.bordaBox .b3 {
	margin: 0 2px;
}

.bordaBox .b4 {
	height: 2px;
	margin: 0 1px;
}

.bordaBox .conteudo {
	padding: 0px 10px 0px 10px;
	display: block;
	background: #356AA0;
	border-left: 0px solid #356AA0;
	border-right: 1px solid #356AA0;
}

