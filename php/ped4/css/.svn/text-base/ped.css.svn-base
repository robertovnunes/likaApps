.menup, .menuh, .abais, .menu-hv { position: relative; margin: 0; padding: 0; display: block; zoom: 1;} 
.menup *, .menuh *, .abais *, .menu-hv * { margin: 0; padding: 0 ; list-style: none }
.menup li, .menuh li, .abais li, .menu-hv li { position: relative; line-height: 2em; vertical-align: top; }
.menup a{ display: block; zoom: 1; line-height: 2.2em }
.menup li ul { left: 100%; top: 0; }

.menuh a, .abais a { display: block; zoom: 1; }
.menuh:after, .menuh.after, .abais.after, .menu-hv:after, .menu-hv.after
{ content: "."; line-height: 0px; clear: both; display: block; visibility: hidden }
.menuh li, .abais li, .menu-hv li{ float: left; cursor:pointer; font-weight:bold; color:#FFFFFF; text-align:center }  
.menuh li { width: 90px; margin-right:3px; }
.abais li { width: 114px; margin-right:1px; margin-bottom:1px }  

/*.menuh li, .abais li { width: 90px; cursor:pointer; font-weight:bold; color:#FFFFFF; margin-right:3px;} */
.menuh .msel a, .abais .msel a{ background-color: #D9E5F1; color:#000000;}
.menu-hv .msel {background-color:#496284;}
.menuh a, .abais a { background-color: #629BD2;}

.menup li, .menu-hv li { width: 145px; background-color: #629BD2; cursor:pointer;} 
.menu-hv li { width: 150px;}
.menup a { background-color: #D9E5F1; margin:2px;}
.menup li a:hover { background-color: #629BD2; } /*98-155-194*/
.menu-hv li:hover, .menu-hv li.hover { color:#000000;}

.menu-hv li { float: left; }  
.menu-hv li ul li { float: none; }
.menu-hv li ul li ul { position: absolute; left: 100%; top: 0; }
.menu-hv li ul, .menu-hv li:hover ul ul, .menu-hv li.hover ul ul{ position: absolute; visibility: hidden; }
.menu-hv li:hover ul,  .menu-hv li.hover ul, .menu-hv li li:hover ul, .menu-hv li li.hover ul { visibility: visible }


li font{
	color: #FFFFFF;
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size:10px;
	font-weight:bold;
}

li a:link, li a:visited, li a:active, li a:hover {
	color: #000000;
	font-weight:bold;
	text-decoration: none;
}

/* Micox Pseudo-class-css2 to IE (MXPC). Activate .hover and .first-child in IE 6
   http://elmicox.blogspot.com/2008/03/ativando-hover-e-first-child-no-ie-6-um.html */
* html * { color: expression( (function(who){ if(!who.MXPC){
 who.MXPC = '1';
 if(who.nodeName != 'A'){
  who.onmouseenter=function(){ who.className += ' hover'};
  who.onmouseleave=function(){ who.className = who.className.replace(' hover','')}; }
 (who==who.parentNode.firstChild) ? who.className += ' first-child' : '' ;
} } )(this) , 'auto') }
