package model.paciente.prontuario.atendimento;

public enum TipoAtendimento {
	ADMISSAO, READMISSAO, EVOLUCAO;
	
	public String toString() {
		if(this.equals(ADMISSAO)){
			return "admissao";
		}else if(this.equals(READMISSAO)){
			return "readmissao";
		}else if(this.equals(EVOLUCAO)){
			return "evolucao";
		}else{
			return null;
		}
	};
	
}
