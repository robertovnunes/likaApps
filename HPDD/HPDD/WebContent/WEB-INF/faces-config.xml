<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">

<navigation-rule>
	<from-view-id>*</from-view-id>
	<navigation-case>
		<from-outcome>newUser</from-outcome>
		<to-view-id>/publico/signup.xhtml</to-view-id> <!-- fazer admin para  mostrar -->
	</navigation-case>
	<navigation-case>
		<from-outcome>userSuccess</from-outcome>
		<to-view-id>/publico/showUser.xhtml</to-view-id> <!-- fazer admin para  mostrar -->
	</navigation-case>
</navigation-rule>

</faces-config>
