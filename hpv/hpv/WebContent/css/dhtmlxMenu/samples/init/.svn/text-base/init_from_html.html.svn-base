			<div id="ms2" type="separator"></div>
			<div id="m15" text="Print" img="print.gif" imgdis="print_dis.gif"><hotkey>Ctrl+P</hotkey></div>
			<div id="m16" text="Page Setup" img="page_setup.gif" imgdis="page_setup_dis.gif" enabled="false"></div>
			<div id="ms3" type="separator"></div>
			<div id="m17" text="Close"><hotkey>Ctrl+Q</hotkey></div>
		</div>
		<!-- edit -->
		<div id="m2" text="Edit">
			<div id="m21" text="Undo" img="undo.gif" imgdis="undo_dis.gif"><hotkey>Ctrl+Z</hotkey></div>
