<script>
	var menu;
	function initMenu() {
		menu = new dhtmlXMenuObject("menuObj");
		menu.setImagePath("../../codebase/imgs/");
		menu.setIconsPath("../images/");
		menu.loadXML("../common/dhtmlxmenu.xml?etc="+new Date().getTime());
	}
	function changeNames() {
		menu.forEachItem(function(id){
			if (menu.getItemType(id) != "separator") {
				menu.setItemText(id, "New Text");
			}
		});
	}
</script>

