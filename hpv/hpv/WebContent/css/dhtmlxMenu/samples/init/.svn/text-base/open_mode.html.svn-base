<script>
	var menu1, menu2;
	function initMenu() {
		// web mode
		menu1 = new dhtmlXMenuObject("menuWeb");
		menu1.setImagePath("../../codebase/imgs/");
		menu1.setIconsPath("../images/");
		menu1.setOpenMode("web");
		menu1.loadXML("../common/dhtmlxmenu.xml?e="+new Date().getTime());
		// win mode
		menu2 = new dhtmlXMenuObject("menuWin");
		menu2.setImagePath("../../codebase/imgs/");
		menu2.setIconsPath("../images/");
		menu2.setOpenMode("win");
		menu2.loadXML("../common/dhtmlxmenu.xml?e="+new Date().getTime());
	}
</script>

