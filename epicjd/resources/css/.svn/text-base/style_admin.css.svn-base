/* rounded */
#nav {
	margin: 0;
	padding: 0 0 20px 10px;
}

#nav li {
	margin: 0;
	padding: 0;
	display: inline;
	list-style-type: none;
}

#nav a:link,#nav a:visited {
	float: left;
	font-size: 10px;
	line-height: 14px;
	font-weight: bold;
	padding: 0 10px 6px 4px;
	text-decoration: none;
	font-size: 8pt;
	color: #708491;
}

#nav a:link.active,#nav a:visited.active,#nav a:hover {
	color: #000;
	/*background: url(rounded.gif) bottom center;*/
	background-image: url(../imagens/rounded.gif);
	background-repeat: no-repeat;
	background-position: bottom;
}

