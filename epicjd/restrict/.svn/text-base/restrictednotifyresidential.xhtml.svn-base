<h:form id="texto">
		<h:messages/>
		<div style="width: 774.5px; height: 276px; overflow: auto;">
		<h:panelGrid width="795" height="30" valign="top" align="left" colspan="2" style="background-color: #4E4E27; color: #FFFFFF; font-family: Arial; font-size: 10 pt; font-weight: bold">
			<h:outputLabel value="Data residence"></h:outputLabel>
		</h:panelGrid>	
		<h:panelGrid columns="2" width="795" height="30" valign="top" align="left" bgcolor="#EBEBD6">
			<h:outputLabel value="Country: " for="country"/>
			<h:inputText id="country" value="#{residentialBean.selected.country}" required="false" maxlength="509"/>	
			<h:outputLabel value="State: " for="state"/>
			<h:inputText id="state" value="#{residentialBean.selected.state}" required="false" maxlength="30"/>
			<h:outputLabel value="City of residence: " for="cityResidence"/>
			<h:inputText id="cityResidence" value="#{residentialBean.selected.cityResidence}" required="false" maxlength="50"/>
			<h:outputLabel value="Code: " for="code"/>
			<h:inputText id="code" value="#{residentialBean.selected.code}" required="false" maxlength="11"/>
			<h:outputLabel value="District: " for="district"/>
			<h:inputText id="district" value="#{residentialBean.selected.district}" required="false" maxlength="50"/>
			<h:outputLabel value="Neighborhood: " for="neighborhood"/>
			<h:inputText id="neighborhood" value="#{residentialBean.selected.neighborhood}" required="false" maxlength="50"/>
			<h:outputLabel value="Sidewalk: " for="sidewalk"/>
			<h:inputText id="sidewalk" value="#{residentialBean.selected.sidewalk}" required="false" maxlength="50"/>
			<h:outputLabel value="Number: " for="number"/>
			<h:inputText id="number" value="#{residentialBean.selected.number}" required="false" maxlength="10"/>
			<h:outputLabel value="Complement: " for="complement"/>
			<h:inputText id="complement" value="#{residentialBean.selected.complement}" required="false" maxlength="50"/>
			<h:outputLabel value="Geographic coordinates: " for="geoCoordinates"/>
			<h:inputText id="geoCoordinates" value="#{residentialBean.selected.geoCoordinates}" required="false" maxlength="11"/>
			<h:outputLabel value="Zip code: " for="zip"/>
			<h:inputText id="zip" value="#{residentialBean.selected.zip}" required="false" maxlength="9"/>	
			<h:outputLabel value="Phone number: " for="phone"/>
			<h:inputText id="phone" value="#{residentialBean.selected.phone}" required="false" maxlength="15"/>	
			<h:outputLabel value="Zone: " for="zone"/>	
			<h:selectOneMenu id="zone" value="#{residentialBean.selected.zone}">
				<c:selectItem itemValue="" itemLabel="Select zone"/>
				<c:selectItem itemValue="Urban" itemLabel="Urban"/>
				<c:selectItem itemValue="Rural" itemLabel="Rural"/>
				<c:selectItem itemValue="Peri" itemLabel="Peri"/>
				<c:selectItem itemValue="Unknown" itemLabel="Unknown"/>
			</h:selectOneMenu>	
				
			<h:outputLabel></h:outputLabel>
			<h:outputLabel>
			<!-- <h:commandLink action="/restrict/restrictednotifyindividual" title="Back">
				<h:commandButton value="Back" action="individual" />
			</h:commandLink> -->
			<h:commandButton  styleClass="botao"  value="Save" action="#{residentialBean.save}" />
			</h:outputLabel>
			<h:outputLabel></h:outputLabel>
			<h:outputLabel></h:outputLabel>
			<h:outputLabel></h:outputLabel>
			<h:outputLabel><h:commandButton styleClass="botao" value="Finish" action="imagesCreated" /></h:outputLabel>
		</h:panelGrid>	
		</div>
	</h:form>

	<br/>
	<img src="../resources/imagens/seq_ind.gif"/>

</div>
