<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:rich="http://richfaces.org/rich"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:a4j="http://richfaces.org/a4j"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:p="http://primefaces.org/ui"
      xmlns:t="http://java.sun.com/jsf/html">
<h:head>
	<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1" />
	<title>Ler Feedbacks</title>
</h:head>
<h:body>
	<f:event listener="#{lerFeedbacks.carregarListaFeedback()}"
		type="javax.faces.event.PreRenderComponentEvent" />
	<rich:panel id="panel_LerFeedback" styleClass="panel_invisivel">
		<h:outputText value="Feedbacks dos aluno" styleClass="campo_titulo" />
		<br />
		<br />
		<h:commandLink id="commandButton_BaixarArquivo" 
			value="Clique aqui para baixar a tabela em Excel"
			actionListener="#{lerFeedbacks.downloadArquivoReferencia()}"
			styleClass="link" />
		<br />
		<br />
		<rich:dataTable id="dataTable_feedbacks"
			value="#{lerFeedbacks.listaFeedbacks}" var="feedback">
			<h:column>
				<f:facet name="header">
					<h:outputText value="Aluno" />
				</f:facet>
				<h:outputText value="#{feedback.aluno.usuario.nome}" />
			</h:column>
			<h:column>
				<f:facet name="header">
					<h:outputText value="Curso" />
				</f:facet>
				<h:outputText value="#{feedback.curso.nome}" />
			</h:column>
			<h:column>
				<f:facet name="header">
					<h:outputText value="Data" />
				</f:facet>
				<h:outputText value="#{feedback.data}" />
			</h:column>
			<h:column>
				<f:facet name="header">
					<h:outputText value="Resposta da primeira pergunta" />
				</f:facet>
				<h:outputText value="#{feedback.respostaUm}" />
			</h:column>
			<h:column>
				<f:facet name="header">
					<h:outputText value="Resposta da segunda pergunta" />
				</f:facet>
				<h:outputText value="#{feedback.respostaDois}" />
			</h:column>
		</rich:dataTable>
		<br />
		<h:commandButton id="commandButton_Voltar" value="Voltar"
			actionListener="#{principalAdmin.apagarConteudo()}"
			render="document.getelementbyid(panelGroup_Target)" />
	</rich:panel>
	<t:outputStylesheet library="css" name="pensae.css" />
</h:body>
</html>